oute\n            path=\"/admin\"\n            component={Admin}\n            fail=\"/login\"\n          ></ProtectedLoginRoute>\n\n          <ProtectedLoginRoute\n            path=\"/\"\n            component={Welcome}\n            fail=\"/login\"\n          ></ProtectedLoginRoute>\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/home/tuan/tuan-trainning/react_login/my-app/src/reportWebVitals.js",[],"/home/tuan/tuan-trainning/react_login/my-app/src/loginPage/index.jsx",["56","57"],"import React, { useState } from \"react\";\nimport \"./style.css\";\nimport axios from \"axios\";\nimport Input from \"../components/input\";\nimport Button from \"../components/button\";\nimport Title from \"../components/titleINIT\";\nimport { useHistory } from \"react-router-dom\";\nimport \"react-notifications/lib/notifications.css\";\nimport {\n  NotificationContainer,\n  NotificationManager,\n} from \"react-notifications\";\nimport Swal from \"sweetalert2\";\nexport default function Login() {\n  const history = useHistory();\n  const [loginInfo, updateLoginInfo] = useState({\n    username: \"\",\n    password: \"\",\n  });\n\n  const handleChange = (key) => {\n    return (val) => {\n      const newLoginInfo = { ...loginInfo }; // copy object\n      newLoginInfo[key] = val; // update key, value\n      // const updatedLoginInfo = { ...loginInfo, [key]: val } // nhu tren\n\n      updateLoginInfo(newLoginInfo);\n    };\n  };\n\n  const handleLogin = () => {\n    // console.log(loginInfo);\n\n    axios({\n      method: \"POST\",\n      url: \"http://localhost:8069/api/login\",\n      data: loginInfo,\n    })\n      .then((res) => {\n        console.log(\"Login success\", res.data);\n\n        let data = res.data;\n\n        localStorage.setItem(\"token\", JSON.stringify(data.token));\n        localStorage.setItem(\"userID\", JSON.stringify(data.id));\n\n        history.push(\"/\");\n      })\n      .catch(() => {\n        console.log(\"error\");\n        NotificationManager.success(\n          \"Please try again after 3 second\",\n          \"Username or Password wrong\",\n          3000\n        );\n      });\n  };\n\n  return (\n    <div className=\"login-page\">\n      <div className=\"form\">\n        <Title></Title>\n        <form className=\"login-form\">\n          <Input\n            placeHolder=\"username\"\n            onChange={handleChange(\"username\")}\n          ></Input>\n          <Input\n            placeHolder=\"password\"\n            onChange={handleChange(\"password\")}\n          ></Input>\n          <Button onClick={handleLogin} Name={\"Login\"}></Button>\n          <p className=\"message\">\n            Not registered? <a href=\"#\">Create an account</a>\n          </p>\n        </form>\n        <NotificationContainer />\n      </div>\n    </div>\n  );\n}\n","/home/tuan/tuan-trainning/react_login/my-app/src/components/input/index.jsx",[],"/home/tuan/tuan-trainning/react_login/my-app/src/components/titleINIT/index.jsx",[],"/home/tuan/tuan-trainning/react_login/my-app/src/components/button/index.jsx",[],"/home/tuan/tuan-trainning/react_login/my-app/src/rootPage/index.jsx",["58"],"import React, { Component } from \"react\";\nimport \"./style.css\";\nimport Button from \"../components/button\";\nimport { useHistory } from \"react-router-dom\";\nimport Header from \"../components/header\";\n\nexport default function Welcome() {\n  const history = useHistory();\n\n  const logOut = () => {\n    history.push(\"/login\");\n    localStorage.removeItem(\"token\");\n  };\n\n  return (\n    <div className=\"welcome\">\n      <Header />\n      <h1>WELCOME HOME</h1>\n      <div className=\"welcome__logout\">\n        <Button Name={\"LogOut\"} onClick={logOut}></Button>\n      </div>\n    </div>\n  );\n}\n","/home/tuan/tuan-trainning/react_login/my-app/src/adminPage/index.jsx",[],"/home/tuan/tuan-trainning/react_login/my-app/src/components/ProtectedRoute/index.jsx",["59","60"],"import React, { Component } from \"react\";\nimport { useHistory, Redirect, Route } from \"react-router-dom\";\n\nexport default function ProtectedRoute(props) {\n  // if (props.renderComp) {\n  //   return localStorage.getItem('token') ? props.renderComp()\n  // : <Redirect to={props.fail} />;\n  // }\n  let isLogin = localStorage.getItem(\"token\");\n  console.log(\"Islogin is: \", isLogin);\n\n  // return isLogin ? (\n  //   <Redirect to={props.success} component={props.component} />\n  // ) : (\n  //   <Redirect to={props.fail} />\n  // );\n  return isLogin ? (\n    <Route path={props.path} component={props.component} />\n  ) : (\n    <Redirect to={props.fail} />\n  );\n}\n","/home/tuan/tuan-trainning/react_login/my-app/src/components/header/index.jsx",["61","62"],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":1,"column":8,"nodeType":"69","messageId":"70","endLine":1,"endColumn":12},{"ruleId":"67","severity":1,"message":"71","line":6,"column":10,"nodeType":"69","messageId":"70","endLine":6,"endColumn":19},{"ruleId":"67","severity":1,"message":"72","line":8,"column":50,"nodeType":"69","messageId":"70","endLine":8,"endColumn":54},{"ruleId":"67","severity":1,"message":"73","line":13,"column":8,"nodeType":"69","messageId":"70","endLine":13,"endColumn":12},{"ruleId":"74","severity":1,"message":"75","line":74,"column":29,"nodeType":"76","endLine":74,"endColumn":41},{"ruleId":"67","severity":1,"message":"77","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":26},{"ruleId":"67","severity":1,"message":"77","line":1,"column":17,"nodeType":"69","messageId":"70","endLine":1,"endColumn":26},{"ruleId":"67","severity":1,"message":"78","line":2,"column":10,"nodeType":"69","messageId":"70","endLine":2,"endColumn":20},{"ruleId":"67","severity":1,"message":"78","line":2,"column":17,"nodeType":"69","messageId":"70","endLine":2,"endColumn":27},{"ruleId":"79","severity":1,"message":"80","line":25,"column":6,"nod